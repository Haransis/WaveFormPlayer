<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SoundWaveView">
        <attr name="playedColor" format="color"/>
        <attr name="nonPlayedColor" format="color"/>
        <attr name="db" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="PlayerView">
        <attr name="mainColor" format="color"/>
        <attr name="secondaryColor" format="color"/>
        <attr name="title" format="string"/>
        <attr name="waveDb" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RecView">
        <attr name="recview_color" format="color"/>
        <attr name="recview_playedColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="RecPlayerView">
        <attr name="rec_color" format="color"/>
        <attr name="rec_playedColor" format="color"/>
        <attr name="rec_duration" format="integer"/>
        <attr name="rec_interval" format="integer"/>
    </declare-styleable>
</resources>